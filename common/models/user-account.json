{
  "name": "user_account",
  "plural": "user_accounts",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "first_name": {
      "type": "string",
      "required": false
    },
    "last_name": {
      "type": "string",
      "required": false
    },
    "email": {
      "type": "string",
      "required": true
    },
    "facebook_id": {
      "type": "string",
      "required": false
    },
    "gmail_id": {
      "type": "string",
      "required": false
    },
    "password": {
      "type": "string",
      "required": true
    },
    "role": {
      "type": "string",
      "required": false
    },
    "gender": {
      "type": "string",
      "required": true,
      "default": "none"
    },
    "phone_no": {
      "type": "number",
      "required": true
    },
    "birth_date": {
      "type": "date",
      "required": false
    },
    "is_active": {
      "type": "boolean",
      "required": false
    },
    "created_date": {
      "type": "date",
      "required": false
    },
    "modified_date": {
      "type": "date",
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "carts": {
      "type": "hasOne",
      "model": "cart",
      "foreignKey": ""
    },
    "images": {
      "type": "hasOne",
      "model": "image",
      "foreignKey": ""
    },
    "addresses": {
      "type": "hasMany",
      "model": "address",
      "foreignKey": ""
    },
    "shoppingcarts": {
      "type": "hasMany",
      "model": "shoppingcart",
      "foreignKey": "userId"
    }
  },
  "acls": [
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "accessTokens"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "ALLOW",
      "property": "createAccessToken"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "AdminLogin"
    }
  ],
  "methods": {}
}
